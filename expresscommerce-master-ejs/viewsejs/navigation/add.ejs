<%- include('../layouts/header') %>

<h3>Add Left Navigation Module</h3>
<div class="row">
    <form action="/navigation/add" method="POST" class="col s12">
    <input type="hidden" name="_csrf" value="<%=csrfToken %>">
        <div class="row">
            <div class="input-field">
                <input type="text" id="code" name="code" >
                <label for="title">Code</label>
            </div>
        </div>
         <div class="row">
            <div class="input-field">
                <input type="text" id="name" name="name" >
                <label for="title">Name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="url" name="url" >
                <label for="title">landing Url</label>
            </div>
        </div>

        <div class="row">
            <div class="input-field">
                <input type="text" id="cssclassname" name="cssclassname" >
                <label for="title">Class Name</label>
            </div>
        </div>

        <div class="row">
            <div class="input-field">
                <input type="number" id="position" name="position" >
                <label for="title">Position</label>
            </div>
        </div>

                <div class="row">
            <input type="submit" value="Save" class="btn">
            <a href="/dashboard" class="btn orange">Cancel</a>
        </div>
    </form>
</div>
<%- include('../layouts/footer') %>