<%- include('../layouts/header') %>
<h3>Edit Left Navigation</h3>
<div class="row">
    <form action="/navigation/<%=modules._id%>" method="post" class="col s12">
    <input type="hidden" name="_csrf" value="<%=csrfToken%>">
        <div class="row">
            <div class="input-field">
                <input type="text" id="code" name="code" value="<%=modules.code%>">
                <label for="title">Code</label>
            </div>
        </div>
         <div class="row">
            <div class="input-field">
                <input type="text" id="name" name="name" value="<%=modules.name%>">
                <label for="title">Name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="url" name="url" value="<%=modules.url%>">
                <label for="title">landing Url</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="text" id="cssclassname" name="cssclassname" value="<%=modules.cssclassname%>">
                <label for="title">Class Name</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field">
                <input type="number" id="position" name="position" value="<%=modules.position%>" >
                <label for="title">Position</label>
            </div>
        </div>
        <div class="row">
            <input type="submit" value="Save" class="btn">
            <a href="/dashboard" class="btn orange">Cancel</a>
        </div>
    </form>
</div>
<%- include('../layouts/footer') %>